#compdef psqlf

_psql_service() {
  #echo "context\n$curcontext\n"
  #local curcontext="$curcontext"
  #typeset -A opt_args
  #_complete_help

  #_arguments 'service=:psql_custom_services'
  _arguments -C \
    '*:service=:_psql_custom_services'
}

_psql_custom_services() {
  local -a psql_services

  psql_services=($(sed -n 's/\[\(.*\)\]/\1/p' ~/.pg_service.conf))

  _describe 'service' psql_services
}

_psql_service
